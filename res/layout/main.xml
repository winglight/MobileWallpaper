<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:androidsam="http://schemas.android.com/apk/res/net.yihabits.mobile.wallpaper"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical">
	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent" >
		<com.omdasoft.mobile.wallpaper.MyWebView android:id="@+id/mainWebView" android:layout_width="fill_parent" android:layout_height="fill_parent"></com.omdasoft.mobile.wallpaper.MyWebView>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:id="@+id/ad_layout" android:layout_height="wrap_content"
		android:gravity="bottom" android:layout_alignParentBottom="true"
		android:layout_alignBottom="@+id/home_layout">
		</LinearLayout>
</RelativeLayout>
